{% extends "@BirkNews/home.html.twig" %}

{% block content %}

    {% for n in news %}
        <div id="news">
            <img src="{{ n.image }}">
            <h2>{{ n.titre }}</h2>
            <p>{{ n.texte }}</p>
            <a href="{{ n.href }}">lire la suite</a>
        </div>
    {% endfor %}

{% endblock %}